	export LDFLAGS="$${LDFLAGS}";\
	make $(MAKEOPTS) all -C $(BUILDROOT)/efl/compile;\
	else\
		echo "efl is up to date.";\
	fi
	$(V)if test ! -f $(BUILDROOT)/efl/stamps/compile -o -f $(BUILDROOT)/efl/stamps/compile-updated; then\
